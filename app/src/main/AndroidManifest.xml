        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Countries" />
        <activity android:name=".Capitals" />
        <activity android:name=".Currencies" />
        <activity android:name=".CallingCodes" />
        <activity android:name=".Population" />
        <activity android:name=".Lat_long" />
        <activity android:name=".Borders" />
        <activity android:name=".Languages" />
        <activity android:name=".Quiz"></activity>
    </application>

</manifest>